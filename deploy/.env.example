# n8n Deployment Configuration
# Copy this file to .env and fill in your values

# Your domain (configured in Cloudflare tunnel)
N8N_HOST=n8n.yourdomain.com

# REQUIRED: Generate with: openssl rand -hex 32
# This encrypts credentials stored in n8n
N8N_ENCRYPTION_KEY=

# Timezone
TIMEZONE=America/Los_Angeles

# Workflow environment variables
JIRA_BASE_URL=https://yourcompany.atlassian.net
JIRA_ACCOUNT_ID=
DISCORD_WEBHOOK_URL=
TOGGL_PROJECT_ID=
TOGGL_WORKSPACE_ID=

# Job Search Digest workflow variables
ADZUNA_APP_ID=
ADZUNA_API_KEY=
GOOGLE_DRIVE_RESUME_FILE_ID=
JOB_SEARCH_DISCORD_WEBHOOK_URL=

# Optional: Basic auth (uncomment in docker-compose.yml to enable)
# N8N_BASIC_AUTH_USER=admin
# N8N_BASIC_AUTH_PASSWORD=
